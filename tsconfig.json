{
  "compilerOptions": {
    "strict": true,

    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,

    "noEmit": true,

    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,

    "target": "es2021",

    "skipLibCheck": true
  }
}
